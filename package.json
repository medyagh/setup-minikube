{
  "name": "minikube-action",
  "version": "0.0.4",
  "private": true,
  "description": "start a kubernetes cluster in github actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build src/main.ts -o dist -d && ncc build src/cleanup.ts -o dist/cleanup -d",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medyagh/setup-minikube"
  },
  "keywords": [
    "minikube",
    "minikube CI",
    "test aginst kubernetes",
    "test kubernetes",
    "kuberentes CI",
    "kubernetes",
    "kubernetes cluster",
    "github",
    "actions"
  ],
  "author": "medyagh",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.18.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.1",
    "jest-circus": "^28.1.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
